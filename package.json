{
    "type": "module",
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,css,md}": "prettier --write"
    },
    "scripts": {
        "prepare": "husky install",
        "lint": "eslint --cache --fix src",
        "prettier": "prettier --write src",
        "compile": "tsc && bun run copy",
        "clean": "rimraf dist/",
        "copy": "bunx copyfiles -u 1 src/.env src/**/*.json dist/",
        "build": "bun run lint && bun run prettier && bun run clean && bun run compile",
        "start": "bunx copyfiles -u 1 src/.env src/**/*.json dist/ && cd dist && bun . && cd ..",
        "dev": "bun run clean && tsc-watch --onSuccess \"bun run start\"",
        "prod": "bun run build && bun run start"
    },
    "dependencies": {
        "@discordjs/rest": "^2.5.0",
        "bun": "^1.2.13",
        "discord-api-types": "^0.38.12",
        "discord.js": "^14.19.3",
        "dotenv": "^16.5.0"
    },
    "devDependencies": {
        "@tsconfig/node20": "^20.1.5",
        "@types/bun": "^1.2.13",
        "@types/dotenv": "^8.2.3",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^8.32.1",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.5",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "tsc-watch": "^6.3.1",
        "typescript": "^5.8.3"
    }
}
